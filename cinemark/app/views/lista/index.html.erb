<h1>Listing lista</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lista.each do |listum| %>
      <tr>
        <td><%= listum.nombre %></td>
        <td><%= listum.rating %></td>
        <td><%= link_to 'Show', listum %></td>
        <td><%= link_to 'Edit', edit_listum_path(listum) %></td>
        <td><%= link_to 'Destroy', listum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listum', new_listum_path %>
